        "concurrently": "^5.0.2",
        "cross-env": "^6.0.3",
        "dotenv": "^8.2.0",
        "emotion-normalize": "^10.1.0",
        "mapbox-gl": "^1.6.0",
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "react-map-gl": "^5.1.3",
        "react-router-dom": "^5.1.2",
