{
    "name": "react-boilerplate",
    "version": "1.0.0",
    "description": "React boilerplate for personal projects and technical challenges",
    "main": "src/index.tsx",
    "author": "Lauren Greenway",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/laurengreenway/react-boilerplate.git"
    },
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --open",
        "lint": "eslint ./src/**/**.tsx",
        "lint:fix": "eslint ./src/**/**.tsx --fix",
        "test": "mocha -r test/env",
        "start": "concurrently \"npm run build\" \"npm run dev\""
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.11.0",
        "@typescript-eslint/parser": "^2.11.0",
